        return 120;
      else
        return 172;
    }


    
    // convert an IP to an integer that can be used for sorting
    function ipToInteger(ip) {
      if (ip != null && ip != "") {
        var parts = ip.split('.');

        if (parts.length != 4) {
          ensemble.debug('Invalid IP address');
          return 0;
        } else {
          return (parseInt(parts[0]) * 256 * 256 * 256) +
            (parseInt(parts[1]) * 256 * 256) +
            (parseInt(parts[2]) * 256) +
            parseInt(parts[3]);
        }
      } else {
        return 0;
      }
    }


    function getPortSpeedDropdownItems (max) {
      
      var allOptions = [
        { "value": -1, "label": "Auto" },
        { "value": 10, "label": "10 Mbps" },
        { "value": 100, "label": "100 Mbps" },
        { "value": 1000, "label": "1 Gbps" },
        { "value": 2500, "label": "2.5 Gbps" },
        { "value": 5000, "label": "5 Gbps" },
        { "value": 10000, "label": "10 Gbps" }
      ];

      var deviceOptions = [];

      for (var i = 0; i < allOptions.length; i++) {
        if (allOptions[i].value <= max)
          deviceOptions.push(allOptions[i]);
      }

      return deviceOptions;
    }

    function getDeviceGlobalIpv6List (devices, mac) {
      var dropdownList = [];
      devices.map(function(d) {
        if (d.Key == mac) {
          if (d.IPv6Address != null && d.IPv6Address.length > 0) {
            d.IPv6Address.map(function(ip) {
              // filter out Local IPs
              if (ip.Scope == "global" && ip.Address != null)
                dropdownList.push(ip.Address);
            });
          }
        }
      });

      return dropdownList;
    }


    function getDeviceMstId(macAddress) {
      var result = '';
      var macAddressArr = macAddress.split("");
      for (var i = 0; i < macAddressArr.length; i++) {
          if (macAddressArr[i] != ':') {
              result += macAddressArr[i];
          }
      }
      return result;
    }

    function addColonsToMac(macWithoutColons) {
      var macWithColons = '';
      for (var i = 0; i < macWithoutColons.length; i += 2) {
          macWithColons += macWithoutColons.substr(i, 2);
          if (i < macWithoutColons.length - 2) {
              macWithColons += ':';
          }
      }
      return macWithColons.toUpperCase();
    }

    
