    function convertUtcToGmtPlus1(utcTimestamp) {
      // Create a Date object from the UTC timestamp
      var date = new Date(utcTimestamp);

      // Get the UTC hours
      var utcHours = date.getUTCHours();

      // Check if the date is during daylight saving time
      var isDaylightSavingTime = isDST(date);

      // Adjust hours for GMT+1
      var gmtPlus1Hours = utcHours + 1;

      // If it's daylight saving time, add an additional hour
      if (isDaylightSavingTime) {
          gmtPlus1Hours += 1;
      }

      // Set the hours to GMT+1
      date.setUTCHours(gmtPlus1Hours);

      // Format the date string as desired (YYYY-MM-DD HH:mm:ss)
      var gmtPlus1String = formatDate(date);

      return gmtPlus1String;
    }


    // Function to format the date string as "YYYY-MM-DD HH:mm:ss"
    function formatDate(date) {
        var year = date.getUTCFullYear();
        var month = padZero(date.getUTCMonth() + 1); // Adding 1 because getUTCMonth() returns zero-based month
        var day = padZero(date.getUTCDate());
        var hours = padZero(date.getUTCHours());
        var minutes = padZero(date.getUTCMinutes());
        var seconds = padZero(date.getUTCSeconds());

        return year + '-' + month + '-' + day + ' ' + hours + ':' + minutes + ':' + seconds;
    }

    // Function to pad a number with leading zeros if it's less than 10
    function padZero(num) {
        return (num < 10 ? '0' : '') + num;
    }

    // Function to check if a date is during daylight saving time in the Netherlands
    function isDST(date) {
        var year = date.getUTCFullYear();
        var dstStart = new Date(Date.UTC(year, 2, 31, 1)); // Last Sunday in March
        var dstEnd = new Date(Date.UTC(year, 9, 31, 1)); // Last Sunday in October

        // If the date is between the last Sunday in March and the last Sunday in October, it's daylight saving time
        return date >= dstStart && date < dstEnd;
    }

    function formatBytes(bytes) {
      if (bytes == null)
        return "0";

      if (bytes == 0)
        return "0";

      var converstionToGB = 1073741824;
      var GB = 0;
      if (bytes > 0) {
        GB = Math.round(bytes / converstionToGB * 100) / 100;
        return GB + ' GB';
      }
    }

    function convertUtcToGmtPlus1TimeOnly(utcTime) {
      var utcDate = new Date(utcTime);
      // Get UTC components
      // add one hour for GMT+1
      var hours = utcDate.getUTCHours() + 1; 
      var minutes = utcDate.getUTCMinutes();
      var seconds = utcDate.getUTCSeconds();
      
      if (hours < 10)
        hours = "0" + hours.toString();
      if (minutes < 10)
        minutes = "0" + minutes.toString();
      if (seconds < 10)
        seconds = "0" + seconds.toString();

      var gmtPlus1Date = new Date( year, month, day, hours, minutes, seconds );
      
      return hours + ":" + minutes + ":" + seconds;
    }


    function formatSpeed(speed) {
      var s = "-";
      
      if (speed >= 1000)
        s = (speed / 1000) + ' Gbps';
      else if (speed >= 0)
        s = speed + ' Mbps';
      else
        s = '0 Mbps';

      return (s);
    }


